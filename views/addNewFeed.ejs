
<%- include('components/header') -%>

    <h1>New Message Page</h1>
    <a class="btn btn-success" href="/">Back Home</a>

    <form method="POST" action="/new-feed" class="container4">
        <div class="mb-3">
            <label for="titleInputEmail1" class="form-label">Name</label>
            <input name="name" type="text" class="form-control" id="titleInputEmail1" aria-describedby="titleHelp">
        </div>
        <div class="mb-3">
            <label for="descriptionInputEmail1" class="form-label">Message</label>
            <input name="message" type="text" class="form-control" id="descriptionInputEmail1" aria-describedby="titleHelp">
        </div>
        <button type="submit" class="btn btn-primary">submit</button>
    </form>




